[menu __print Ercf]
name: {{ gettext('ERCF') }}
icon: ercf
enable: {{ (printer.pause_resume.is_paused == True) and (printer.extruders.count > 0) }}

[menu __print Ercf HOMEERCF]
name: {{ gettext('Home ERCF') }}
icon: home
method: printer.gcode.script
params: {"script":"ERCF_HOME"}

